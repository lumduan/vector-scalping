# Vector Scalping Strategy Environment Configuration
# Copy this file to .env and fill in your actual values

# Trading Configuration
# SYMBOL=EURUSD
# EXCHANGE=FX_IDC

# TradingView/tvkit Configuration (if needed)
# TRADINGVIEW_USERNAME=your_username
# TRADINGVIEW_PASSWORD=your_password

# API Keys (if using external data providers)
# API_KEY=your_api_key_here
# API_SECRET=your_api_secret_here

# Database Configuration (if storing trade history)
# DATABASE_URL=postgresql://user:password@localhost/trading_db

# Logging Configuration
# LOG_LEVEL=INFO
# LOG_FILE=trading.log

# Risk Management
# MAX_POSITION_SIZE=10000
# DAILY_LOSS_LIMIT=500

# Notification Settings (if using alerts)
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token
# TELEGRAM_CHAT_ID=your_chat_id
# SLACK_WEBHOOK_URL=your_slack_webhook_url

# Paper Trading / Live Trading Flag
# LIVE_TRADING=false
# PAPER_TRADING=true
